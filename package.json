{
  "name": "danger-plugin-code-coverage",
  "description": "Danger.JS plugin to display the code coverage on a pull request by commenting it via the CI",
  "author": {
    "name": "Guillaume Piedigrossi",
    "email": "guillaume.piedigrossi@gadz.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spoutnik97/danger-plugin-code-coverage.git"
  },
  "bugs": {
    "url": "https://github.com/Spoutnik97/danger-plugin-code-coverage/issues"
  },
  "homepage": "https://github.com/Spoutnik97/danger-plugin-code-coverage#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "coverage",
    "quality"
  ],
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --parser typescript --no-semi --trailing-comma es5 --write --print-width 120",
    "prettier-project": "npm run prettier-write -- 'src/**/*.{ts,tsx}'",
    "lint": "tslint \"src/**/*.ts\""
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "byzantine": "^1.1.0",
    "danger": "^10.1.1",
    "lodash": "^4.17.15",
    "markdown-table": "^2.0.0"
  },
  "devDependencies": {
    "ts-jest": "^20.0.0",
    "@types/jest": "^19.2.4",
    "tslint": "^5.4.3",
    "danger": "*",
    "cz-conventional-changelog": "^2.0.0",
    "jest": "^20.0.1",
    "prettier": "^1.3.1",
    "semantic-release": "^6.3.6",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "esdoc": "^0.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  }
}
